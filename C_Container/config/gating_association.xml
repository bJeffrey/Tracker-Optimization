<?xml version="1.0" encoding="UTF-8"?>
<GatingAssociation version="1.0">
  <CourseGates>
    <Enabled>true</Enabled>
    <RadiusMeters>50000</RadiusMeters>
    <SurfaceGateSideMeters x="253.44" y="253.44" z="3351.38"/>
  </CourseGates>

  <CoarseGating>
    <!-- Very fast gate before any statistical gating/association -->
    <Enabled>true</Enabled>
    <!-- Example: AABB overlap / range-only prefilter, etc. -->
    <AabbInflateMeters>500</AabbInflateMeters>
  </CoarseGating>

  <FineGating>
    <Enabled>true</Enabled>
    <!-- Example: chi-square threshold for position residual in 3D -->
    <ChiSquareThreshold dof="3">16.27</ChiSquareThreshold> <!-- ~99.9% for dof=3 -->
  </FineGating>

  <Association>
    <Method>GNN</Method>
    <Decision>
      <ElapsedBudgetFrac>0.7</ElapsedBudgetFrac>
      <AvgPairsPerMeasLow>1.5</AvgPairsPerMeasLow>
      <AmbiguousFracLow>0.2</AmbiguousFracLow>
      <AvgPairsPerMeasHigh>4.0</AvgPairsPerMeasHigh>
      <AmbiguousFracHigh>0.5</AmbiguousFracHigh>
      <PmbmElapsedBudgetFrac>0.5</PmbmElapsedBudgetFrac>
      <PmbmCpuMax>0.7</PmbmCpuMax>
      <FalseAlarmHigh>0.3</FalseAlarmHigh>
      <CpuHardMax>0.9</CpuHardMax>
    </Decision>
  </Association>

  <HitMissPolicy>
    <OnHit>
      <ResetMissCount>true</ResetMissCount>
      <QualityBoost>1.0</QualityBoost>
    </OnHit>

    <OnMiss>
      <IncrementMissCount>true</IncrementMissCount>
      <MaxMissesBeforeDelete>10</MaxMissesBeforeDelete>
      <QualityDecay>0.9</QualityDecay>
    </OnMiss>
  </HitMissPolicy>
</GatingAssociation>
